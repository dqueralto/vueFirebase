AAEA,YAAA,KAAA,EAAA,QAAA,CAAA,IAAA,GAAA,WAAA,CAAA,KAFA;AAGA,YAAA,YAAA,EAAA;AACA,cAAA,IAAA,EAAA,KAAA,CAAA,YAAA,CAAA,IADA;AAEA,cAAA,GAAA,EAAA,KAAA,CAAA,YAAA,CAAA;AAFA;AAHA,WAAA;AAQA,SAbA;AAcA,aAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA,aAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA;AAEA,WAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA,KA1BA;AA2BA,IAAA,UAAA,EAAA,oBAAA,IAAA,EAAA;AACA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,WAAA,EAAA,GAAA,CAAA,IAAA,CAAA,EAAA,EAAA,MAAA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,EAAA;AACA,KAhCA;AAiCA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AACA,MAAA,QAAA,CAAA,IAAA,GAAA,OAAA,GAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA;AACA,OAFA;AAGA;AArCA,GAxBA;AA+DA,EAAA,QAAA,EAAA;AA/DA,CAAA","sourcesContent":["<template lang=\"html\">\n<div>\n    <nav class=\"navbar navbar-dark bg-primary\">\n        <a href=\"/\" class=\"navbar-brand\">Ejemplo Vue+Firebase</a>\n        <button type=\"button\" class=\"btn btn-secondary ml-auto\" v-on:click=\"logout\">Log out</button>\n    </nav>\n    <div class=\"container\">\n        <div class=\"row mt-5\">\n            <div class=\"col-sm-4\">\n                <div class=\"card\" style=\"width: 18rem;\">\n                    <div class=\"card-header\">\n                        Agrega una nueva tarea\n                        <div class=\"card-body\">\n                            <input type=\"text\"  v-model=\"nuevaNota.Texto\" placeholder=\"Texto\">\n                            <button type=\"submit\" class=\"btn btn-primary\">Guardar nota</button>\n                            <input type=\"file\" @change=\"archivoSeleccionado\" />\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-sm-8 text-center\">\n                <div class=\"card\" style=\"width: 18rem;\">\n                    <div class=\"card-header\">\n                        Lista Tareas\n                        <div class=\"card-body\">\n                            <ul class=\"list-group\" v-for=\"note in listaNotas\" :key=\"note.Texto\">\n                                <li class=\"list-group-item\">{{note.Texto}}</li>\n                                <li class=\"list-group-item\">{{note.Autor}}</li>\n                                <li class=\"list-group-item\"><a :href=\"note.archivoSubir.url\">{{note.archivoSubir.url}}</a></li>\n                                <input type=\"button\" class=\"btn btn-primary\" v-on:click=\"borrarNota(note)\">Borrar</input>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n</template>\n\n<script lang=\"js\">\nimport {\n    db\n} from '../db.js'\n\nimport firebase from 'firebase';\n\nexport default {\n    name: 'privado',\n    props: [],\n    mounted() {\n\n    },\n    components: {},\n    data() {\n        return {\n            listaNotas: [],\n            nuevaNota: {\n                Texto: \"\",\n                Autor: \"\",\n                archivoSubir: {\n                    name: null,\n                    url: null\n                }\n\n            }\n        }\n    },\n    firestore: {\n        listaNotas: db.collection('listNotas'),\n    },\n    methods: {\n        archivoSeleccionado(event) {\n            this.archivoSubir = event.target.files[0];\n        },\n        anadeNota() {\n            firebase.storage().ref(`${this.archivoSubir.name}`).put(this.archivoSubir); {\n                firebase.storage().ref().child(this.archivoSubir.name).getDownloadURL()\n                    .then(\n                        url => {\n                            console.log(this.nuevaNota.Texto);\n                            this.archivoSubir.url = url\n                            db.collection('listNotas').add({\n                                Texto: this.nuevaNota.Texto,\n                                Autor: firebase.auth().currentUser.email,\n                                archivoSubir: {\n                                    name: this.archivoSubir.name,\n                                    url: this.archivoSubir.url\n                                }\n                            });\n                        })\n                this.nuevaNota.Texto = \"\"\n                this.nuevaNota.Autor = \"\"\n            }\n\n            this.nuevaNota.Texto = \"\"\n            this.nuevaNota.Autor = \"\"\n        },\n        borrarNota: function (note) {\n            /* PARRA BORRAR CAD NOTA METODO DELETE */\n            db.collection('listNotas').doc(note.id).delete()\n            this.nuevaNota.Texto = \"\"\n            this.nuevaNota.Autor = \"\"\n        },\n        logout: function () {\n            firebase.auth().signOut().then(() => {\n                this.$router.replace('login')\n            })\n        }\n    },\n    computed: {\n\n    }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/components"}]}